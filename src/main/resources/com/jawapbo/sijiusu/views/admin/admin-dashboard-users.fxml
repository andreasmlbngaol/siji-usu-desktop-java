<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #D6C0B3;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jawapbo.sijiusu.views.AdminDashboardUserController">
    <children>
        <VBox fillWidth="false" prefHeight="720.0" prefWidth="1280.0" spacing="25.0">
            <children>
                <HBox alignment="CENTER" prefHeight="75.0" prefWidth="1280.0" style="-fx-background-color: #493628; -fx-padding: 12 14;">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <children>
                        <Label style="-fx-text-fill: white;" text="SIJI USU JEUS" textFill="WHITE">
                            <font>
                                <Font name="Segoe UI Bold" size="30.0" />
                            </font>
                        </Label>
                        <Region prefHeight="51.0" prefWidth="911.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="profilButton" onAction="#onProfile" mnemonicParsing="false" style="-fx-background-color: tranparent;" text="Profil" textFill="#d6c0b3">
                            <font>
                                <Font name="Segoe UI Bold" size="15.0" />
                            </font>
                        </Button>
                        <Button fx:id="keluarButton" onAction="#onKeluar" mnemonicParsing="false" style="-fx-background-color: transparent;" text="Keluar" textFill="#d6c0b3">
                            <font>
                                <Font name="Segoe UI Bold" size="15.0" />
                            </font>
                        </Button>
                    </children>
                    <effect>
                        <DropShadow />
                    </effect>
                </HBox>
                <HBox fillHeight="false" prefHeight="65.0" prefWidth="1282.0">
                    <children>
                        <Button fx:id="backtodashboardButton" onAction="#onBack" mnemonicParsing="false" prefHeight="67.0" prefWidth="284.0" style="-fx-background-color: transparent;" text="← Kembali ke dashboard admin">
                            <font>
                                <Font name="Segoe UI Bold" size="15.0" />
                            </font>
                        </Button>
                        <HBox alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="1034.0">
                            <children>
                                <Label text="Detail Manajemen Pengguna">
                                    <font>
                                        <Font name="Segoe UI Bold" size="25.0" />
                                    </font>
                                </Label>
                            </children>
                        </HBox>
                    </children>
                </HBox>
                <VBox alignment="CENTER" prefHeight="179.0" prefWidth="1178.0" style="-fx-background-color: #493628; -fx-border-color: #493628; -fx-border-radius: 12; -fx-background-radius: 12;">
                    <VBox.margin>
                        <Insets left="50.0" right="50.0" />
                    </VBox.margin>
                    <effect>
                        <DropShadow blurType="GAUSSIAN" />
                    </effect>
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Button fx:id="dosenButton" onAction="#onDosen" alignment="CENTER" mnemonicParsing="false" prefHeight="86.0" prefWidth="1176.0" style="-fx-background-color: transparent;" text="Dosen" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="25.0" />
                           </font>
                        </Button>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="329.0" text="Kelola Akun Dosen" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="15.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
                </VBox>
            <VBox alignment="CENTER" prefHeight="179.0" prefWidth="1178.0" style="-fx-background-color: #493628; -fx-border-color: #493628; -fx-border-radius: 12; -fx-background-radius: 12;">
               <effect>
                  <DropShadow blurType="GAUSSIAN" />
               </effect>
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Button fx:id="mahasiswaButton" onAction="#onMahasiswa" alignment="CENTER" mnemonicParsing="false" prefHeight="86.0" prefWidth="1176.0" style="-fx-background-color: transparent;" text="Mahasiswa" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="25.0" />
                           </font>
                        </Button>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="24.0" prefWidth="329.0" text="Kelola Akun Mahasiswa" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Segoe UI Bold" size="15.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets left="50.0" right="50.0" />
               </VBox.margin>
            </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
