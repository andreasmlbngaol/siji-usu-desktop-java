<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #D6C0B3;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jawapbo.sijiusu.views.students.StudentEnrollController">
    <children>
      <VBox layoutX="-9.0" layoutY="-9.0" prefHeight="720.0" prefWidth="1280.0">
         <children>
                  <!-- Header -->
                  <HBox alignment="CENTER" prefHeight="75.0" prefWidth="1280.0" style="-fx-background-color: #493628; -fx-padding: 12 14;">
                      <opaqueInsets>
                          <Insets />
                      </opaqueInsets>
                      <children>
                          <Label style="-fx-text-fill: white;" text="SIJI USU JEUS" textFill="WHITE">
                              <font>
                                  <Font name="Segoe UI Bold" size="30.0" />
                              </font>
                          </Label>
                          <Region prefHeight="51.0" prefWidth="911.0" HBox.hgrow="ALWAYS" />
                          <Button mnemonicParsing="false" onAction="#onSignOut" style="-fx-background-color: transparent;" text="Keluar" textFill="#d6c0b3">
                              <font>
                                  <Font name="Segoe UI Bold" size="15.0" />
                              </font>
                          </Button>
                      </children>
                      <effect>
                          <DropShadow />
                      </effect>
                  </HBox>
              <ScrollPane fitToWidth="true" hbarPolicy="NEVER" prefWidth="1280.0" vbarPolicy="NEVER">
                  <content>
                      <VBox fillWidth="false" prefWidth="1280.0" spacing="20.0" style="-fx-background-color: #D6C0B3;">
                          <children>
      
                              <!-- Navigasi atas -->
                              <HBox fillHeight="false" prefHeight="65.0" prefWidth="1282.0">
                                  <children>
                                      <Button mnemonicParsing="false" onAction="#onBackToDashboard" prefHeight="67.0" prefWidth="284.0" style="-fx-background-color: transparent;" text="← Kembali ke dashboard">
                                          <font>
                                              <Font name="Segoe UI Bold" size="15.0" />
                                          </font>
                                      </Button>
                                      <HBox alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="1034.0">
                                          <children>
                                              <Label text="Detail daftar">
                                                  <font>
                                                      <Font name="Segoe UI Bold" size="25.0" />
                                                  </font>
                                              </Label>
                                          </children>
                                      </HBox>
                                  </children>
                              </HBox>
                        <VBox fx:id="itemsContainer" prefWidth="1280.0" spacing="16.0">
                           <VBox.margin>
                              <Insets left="50.0" right="50.0" />
                           </VBox.margin>
                        </VBox>
      
                              <!-- Tambahkan lebih banyak VBox di sini jika ingin lebih banyak daftar kelas -->
      
                          </children>
                      </VBox>
                  </content>
              </ScrollPane>
         </children>
      </VBox>
    </children>
</AnchorPane>
